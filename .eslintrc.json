{
	"extends": [
		"standard",
		"plugin:react/recommended",
		"prettier"
	],
	"rules": {
		"no-tabs": ["error", { "allowIndentationTabs": true }],
		"indent": ["error", 2, { "SwitchCase": 1 }],
		"no-extra-semi": "error",
		"react/semi": "off",
		"semi": "off",
		"react/prop-types": "off"
	},
	"settings": {
		"react": {
		  "version": "detect"
		}
	}
}
